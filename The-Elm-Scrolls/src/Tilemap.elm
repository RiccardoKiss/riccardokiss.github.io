getTileMap : Level.Level -> Array.Array ( Array.Array Char )
getTileMap lvl =
  case lvl of
    Lvl1 ->
      level2TileMap

    Lvl2 ->
      level2TileMap

    Lvl3 ->
      level2TileMap

getTileTypeFromTileMap : Level.Level -> Float -> Float -> Maybe Char
getTileTypeFromTileMap lvl x y =
    {-
    let
        _ = Debug.log "[getTileTypeFromTileMap] x" (floor x)
        _ = Debug.log "[getTileTypeFromTileMap] y" (floor y)
    in-}
    Array.get (127 - (floor y)) (getTileMap lvl)
    |> Maybe.andThen (Array.get (floor x))
    --|> Debug.log "[getTileTypeFromTileMap] tile"
